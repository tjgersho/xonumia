div(id="setStartSquareModal" class="modal fade" role="dialog")
	div(class="modal-dialog")
		div(class="modal-content")
			div(class="modal-header")
				button(type="button" class="close" data-dismiss="modal") &times;
				h4(class="modal-title") Start Square Text
			div(class="modal-body")
				div(class="form-group")
					label(class="col-md-4 control-label" for="startSquareTextarea") Set Start Square Text 
					div(class="col-md-4")
						textarea(id="startSquareTextarea" name="startSquareTextarea" ng-model="bldCtrl.startSquareTextarea" class="form-control")
							

			div(class="modal-footer")
				button(type="button" class="btn btn-default pull-right" data-dismiss="modal" ng-click="bldCtrl.closeDialog()") Cancel
				button(id="setMapIntro" type="button" class="btn btn-default pull-right"  ng-click="bldCtrl.submitstartSquareText()") Set Start Square Text